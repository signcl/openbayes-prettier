{
  "name": "@openbayes/prettier",
  "version": "0.0.2",
  "description": "OpenBayes prettier configuration",
  "main": "index.js",
  "module": "index.mjs",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    }
  },
  "files": [
    "index.js",
    "index.mjs"
  ],
  "scripts": {
    "get-version": "node -p \"require('./package.json').version\"",
    "commit-version": "git add . && git commit -m \"chore: release v$(pnpm run --silent get-version)\"",
    "release:patch": "pnpm version patch && pnpm commit-version && pnpm publish",
    "release:minor": "pnpm version minor && pnpm commit-version && pnpm publish",
    "release:major": "pnpm version major && pnpm commit-version && pnpm publish"
  },
  "homepage": "https://github.com/signcl/openbayes-prettier#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/signcl/openbayes-prettier.git"
  },
  "keywords": [
    "openbayes",
    "prettier"
  ],
  "author": "lesenelir <hi@lesenelir.me>",
  "license": "MIT",
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "prettier-plugin-packagejson": "^2.5.1",
    "prettier-plugin-sort-json": "^4.0.0",
    "prettier-plugin-tailwindcss": "^0.6.5"
  },
  "devDependencies": {
    "prettier": "^3.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
